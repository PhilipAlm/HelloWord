a. SELECT firstname, lastname FROM students;
b. SELECT firstname, lastname FROM students ORDER BY lastname, firstname;
c. SELECT * FROM students WHERE pNbr like '95%';
d. SELECT firstname,lastname FROM students WHERE mod(substring(pNbr,10,1),2)=0;
e. SELECT COUNT(pNbr) FROM students;
f. SELECT * FROM courses WHERE coursecode LIKE 'FMA%'; alternativt 'FMA___' om det måste vara specefikt 3 tecken efter FMA.
g. SELECT * FROM courses WHERE credits > 7.5;
h. SELECT count(level) FROM courses WHERE level='A/G1/G2';
i. SELECT coursecode FROM takencourses WHER pNbr='910101–1234';
j. SELECT coursename,credits FROM courses JOIN takencourses on (takencourses.coursecode = courses.coursecode) WHERE takencourses.pnbr = '910101-1234';
k. SELECT sum(credits) FROM courses JOIN takencourses on (takencourses.coursecode = courses.coursecode) WHERE takencourses.pnbr = '910101-1234';
l. SELECT avg(grade FROM courses WHERE pnbr='910101-1234';
